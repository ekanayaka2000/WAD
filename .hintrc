{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "meta-viewport": "off",
    "highest-available-document-mode": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-justify",
          "text-justify: inter-word"
        ]
      }
    ]
  }
}